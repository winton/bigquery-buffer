  "devDependencies": {
    "@google-cloud/bigquery": "4.5.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.18",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "expect": "24.9.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
