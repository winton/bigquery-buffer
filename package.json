{
  "name": "bigquery-buffer",
  "version": "0.6.0",
  "description": "BigQuery insert buffer",
  "main": "dist/index.js",
  "scripts": {
    "build": "./bin/build",
    "release": "./bin/release",
    "test": "mocha",
    "update": "ncu -u",
    "watch": "ttab 'tsc -b -w'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/winton/bigquery-buffer.git"
  },
  "author": "Winton Welsh <mail@wintoni.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/winton/bigquery-buffer/issues"
  },
  "homepage": "https://github.com/winton/bigquery-buffer#readme",
  "devDependencies": {
    "@google-cloud/bigquery": "5.5.0",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "expect": "26.6.2",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "mocha": "8.2.1",
    "npm-check-updates": "11.1.1",
    "prettier": "2.2.1",
    "release-it": "14.2.2",
    "semver": "7.3.4",
    "ts-node": "9.1.1",
    "ttab": "0.7.0",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts}": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  }
}
